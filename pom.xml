<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>no.nav.sbl</groupId>
        <artifactId>mor-pom</artifactId>
        <version>2017.10.24.10.37</version>
    </parent>

    <name>veilarbportefolje</name>

    <groupId>no.nav.fo</groupId>
    <artifactId>veilarbportefolje</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <modules>
        <module>web</module>
        <module>config</module>
        <module>jee</module>
    </modules>

    <properties>
        <skip.js-tests>false</skip.js-tests>

        <!-- Egenlagde -->
        <modig-lang.version>1.1.0</modig-lang.version>
        <enhet.tjenestespesifikasjon.versjon>1.0.0</enhet.tjenestespesifikasjon.versjon>
        <common-integrasjonsutils.version>0.0.11</common-integrasjonsutils.version>


        <!-- Tredjeparts -->
        <solr.version>6.4.2</solr.version>
        <vavr.version>0.9.0</vavr.version>
        <okhttp.version>2.7.5</okhttp.version>

        <sonar.exclusions>
            **/mock/*,
            **/config/**,
        </sonar.exclusions>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>no.nav.sbl</groupId>
                <artifactId>bom</artifactId>
                <version>2017.10.24.10.37</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>

            <!-- Virksomhetstjenester -->
            <dependency>
                <groupId>no.nav.meldinger.virksomhet</groupId>
                <artifactId>nav-virksomhet-loependeYtelser-v1-meldingsdefinisjon</artifactId>
                <version>1.0.0</version>
                <classifier>jaxb</classifier>
            </dependency>

            <dependency>
                <groupId>no.nav.meldinger.virksomhet</groupId>
                <artifactId>nav-virksomhet-tiltakOgAktiviteterForBrukere-v1-meldingsdefinisjon</artifactId>
                <version>1.0.0</version>
                <classifier>jaxb</classifier>
            </dependency>

            <dependency>
                <groupId>io.vavr</groupId>
                <artifactId>vavr</artifactId>
                <version>${vavr.version}</version>
            </dependency>

            <!--Dialogarena-->
            <dependency>
                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>common-integrasjonsutils</artifactId>
                <version>${common-integrasjonsutils.version}</version>
            </dependency>

            <!--Solr-->
            <dependency>
                <groupId>org.apache.solr</groupId>
                <artifactId>solr-solrj</artifactId>
                <version>${solr.version}</version>
                <exclusions>
                    <exclusion>
                        <groupId>org.codehaus.woodstox</groupId>
                        <artifactId>wstx-asl</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>


            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>enhet-v1-tjenestespesifikasjon</artifactId>
                <version>${enhet.tjenestespesifikasjon.versjon}</version>
            </dependency>

            <dependency>
                <groupId>com.google.guava</groupId>
                <artifactId>guava</artifactId>
                <version>20.0-hal</version>
            </dependency>
            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbportefolje-config</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.squareup.okhttp</groupId>
                <artifactId>okhttp</artifactId>
                <version>${okhttp.version}</version>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>jsch</groupId>
                <artifactId>jsch</artifactId>
                <version>0.1.44</version>
                <scope>runtime</scope>
            </dependency>

        </dependencies>
    </dependencyManagement>

    <scm>
        <developerConnection>scm:git:http://stash.devillo.no/scm/fo/veilarbportefolje.git</developerConnection>
        <url>http://stash.devillo.no/projects/FO/repos/veilarbportefolje/browse</url>
        <tag>HEAD</tag>
    </scm>

</project>